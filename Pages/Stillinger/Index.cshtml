@page
@model VismaE2interRPWDWEDN.Pages.Stillinger.IndexModel

@{
    ViewData["Title"] = "Index";
    Layout = "_Layout";}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>Index</title>
</head>
<body>
<p>
    <a asp-page="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.Stilling[0].Name)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Stilling[0].PositionStartDate)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Stilling[0].PositionEndDate)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model.Stilling) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Name)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.PositionStartDate)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.PositionEndDate)
            </td>
            <td>
                <a asp-page="./Edit" asp-route-id="@item.ID">Edit</a> |
                <a asp-page="./Details" asp-route-id="@item.ID">Details</a> |
                <a asp-page="./Delete" asp-route-id="@item.ID">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
</body>
</html>
